<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NotasAcademicasApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:NotasAcademicasApp.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="NotasAcademicasApp">

    <ShellContent
        Title="Notas" ContentTemplate="{DataTemplate views:NotasPage}" />
    <ShellContent
        Title="Estudiante" ContentTemplate="{DataTemplate views:EstudiantesPage}"/>
    <ShellContent
        Title="Materias" ContentTemplate="{DataTemplate views:MateriaPage}"/>

</Shell>